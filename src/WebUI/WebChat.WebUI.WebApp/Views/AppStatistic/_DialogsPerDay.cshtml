@model WebChat.WebUI.ViewModels.Statistic.AppStatisticTimePeriodViewModel

<div class="panel panel-default">
    <div class="panel-heading">
        <div class="col-sm-6">
            <h4>Диалогов в день</h4>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                @Html.LabelFor(model => model.StartDate, new { @class = "col-sm-6" })
                @Html.TextBoxFor(model => model.StartDate, new { @class = "col-sm-6" })
            </div>
            <div class="form-group">
                @Html.LabelFor(model => model.EndDate, new { @class = "col-sm-6" })
                @Html.TextBoxFor(model => model.EndDate, new { @class = "col-sm-6" })
            </div>
        </div>
    </div>
    <div class="panel-body">
        <div id="dialogs-per-day-chart">

        </div>
    </div>
</div>
